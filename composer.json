{
    "name": "soban/laravel-er-blueprint",
    "description": "A Laravel package to generate ER diagrams from migrations and models",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Soban\\LaravelErBlueprint\\": "src/"
        },
        "files": [
            "src/Helpers/migration_helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Soban\\LaravelErBlueprint\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Soban\\LaravelErBlueprint\\Providers\\SchemaServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Soban Farooq",
            "email": "sobanfarooq3@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^9.11",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-laravel": "^3.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
